{
      "name": "Miley",
      "description": "A Telethon based Telegram group management bot.",
      "keywords": [
         "Telethon",
         "Telegram",
         "Group",
         "manager",
         "Miley"
      ],   
   "repository": "https://github.com/Amarnathcdj/Miley",
   "addons": [
      {
         "options": {
            "version": "12"
         },
         "plan": "heroku-postgresql"
      }
   ],
   "buildpacks": [
    {
      "url": "heroku/python"
    }     
  ],      
   "env": {
      "TOKEN": {
         "description": "Your bot token. Get one from @BotFather duh",
         "required": true,
         "value": ""
      },
      "API_ID": {
         "description": "Get API_ID from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      },
      "API_HASH": {
         "description": "Get API_HASH from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      },
      "OWNER_ID": {
         "description": "Your user ID as an integer.",
         "required": true,
         "value": ""
      },
      "OWNER_USERNAME": {
         "description": "Your username without the @",
         "value": "RoseLoverX"
      },
      "STRING_SESSION": {
         "description": "Telegram String Session",
         "required": true,
         "value": ""
      },
      "MONGO_DB_URI": {
         "description": "MongoDB Url",
         "required": true,
         "value": ""
      },      
      "BOT_ID": {
         "description": "BOT ID",
         "required": true,
         "value": ""
      },
      "DEV_USERS": {
         "description": "yup",
         "required": false,
         "value": ""
      },
      "SUDO_USERS": {
         "description": "ID of users who are Devs of your bot (can use /py etc.). If you are a noob and would come and bother Saitama support then keep the current ID's here at they are and add yours.",
         "required": false,
         "value": ""
      },
      "UPSTREAM_REPO_URL": {
         "description": "Required for wallpaper. Get your's from https://wall.alphacoders.com/",
         "required": true,
         "value": "https://GitHub.com/Amarnathcdj/Miley"
      }
   },
   "keywords": [
      "telegram",
      "Telethon",
      "group",
      "manager",
      "Miley"
   ],
   "name": "Miley",
   "repository": "https://github.com/Amarnathcdj/Miley"
}
